<duixml>
	<Element resid="PageMain" layoutpos="client" layout="borderlayout()" background="argb(0,0,0,0)">
		<Element id="atom(animator1)" class="animator" layoutpos="left"/>
		<Element layout="borderlayout()" class="page" padding="rect(-50rp,0,0,0)" layoutpos="left">
			<Element layout="flowlayout(0,2,2,0)" padding="rect(65rp,10rp,55rp,0)" layoutpos="left">
				<Element width="250rp" direction="ltr" height="250rp" background="resbmp(1,7,-1,0,0,1,1,library(PageRes.dll))"/>
			</Element>
			<Element layout="borderlayout()" layoutpos="left">
				<Richtext class="headertext" layoutpos="top" content="Welcome"/>
				<Richtext class="instructiontext" layoutpos="top"
				content="Welcome to Rectify11 Installation Wizard. It is recommended to save all your work and close all the apps."/>
				<Richtext class="instructiontext" layoutpos="top"
				content="Please read notes and credits before proceeding, click Next to proceed, Exit to cancel the installation."/>
				<Richtext class="instructiontext" foreground="rgb(236,64,78)" layoutpos="top"
				content="Warning: This is a beta build of Rectify11, do NOT use it on your main PC."/>
				<Element layout="borderlayout()" layoutpos="top">
					<Element layout="flowlayout()" padding="rect(10rp,15rp,0,0)"  layoutpos="left">
						<TouchButton class="NormalButton" id="atom(credits)" height="35rp" width="140rp" content="Credits"/>
					</Element>
				</Element>
			</Element>
		</Element>
	</Element>
</duixml>