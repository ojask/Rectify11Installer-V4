<duixml>
	<Element resid="PageMain" layoutpos="client" layout="borderlayout()" background="argb(0,0,0,0)">
		<Element id="atom(animator1)" class="animator" layoutpos="left"/>
		<Element layout="borderlayout()" class="page" padding="rect(-50rp,0,0,0)" layoutpos="left">
			<Element layout="flowlayout(0,2,2,0)" padding="rect(65rp,10rp,55rp,0)" layoutpos="left">
				<Element width="250rp" direction="ltr" height="250rp" background="sgraphic(1,7,-1,0,0,1,1,library(PageRes.dll))"/>
			</Element>
			<Element layout="borderlayout()" layoutpos="left">
				<Richtext class="headertext" layoutpos="top" content="Before you begin..."/>
				<Richtext class="instructiontext" layoutpos="top"
				content="Windows Defender can cause problems with Rectify11 Installation and potentially breaking your system. Hence it is advised to disable Real Time Protection in Windows Defender. Click Next when ready."/>
				<Element layout="flowlayout()" padding="rect(3rp,15rp,0,0)"  layoutpos="top">
					<TouchButton class="NormalButton" id="atom(opendefender)" content="Open Defender"/>
				</Element>
			</Element>
		</Element>
	</Element>
</duixml>